<div class="container">
  <ul>
    <% @surveys.each do |survey| %>
      <a href='/surveys/<%=survey.id%>'>
        <li class="survey-title"><%= survey.title %></li>
      </a>
        <li class="username">by: <%= user_from_survey(survey).name %>
        <!-- TODO, add link to /users/:id -->
    <% end %>
  </ul>
</div>